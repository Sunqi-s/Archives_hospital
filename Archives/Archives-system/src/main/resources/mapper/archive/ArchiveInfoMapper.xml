    </update>

    <update id="updateArchiveStatusByIds">
        UPDATE archive_info
        SET
        archive_date = CASE
        WHEN archive_status = 0 THEN NOW() -- 修改归档时间
        ELSE archive_date -- 保持原值
        END,
        archive_status = CASE
        WHEN archive_status = 0 THEN 1
        WHEN archive_status = 1 THEN 0
        ELSE archive_status -- 不是0或1，保持原值
        END,
        department = CASE
        WHEN archive_status = 0 THEN #{archiveInfoStatus.department} -- 修改归档部门
        WHEN archive_status = 1 THEN #{archiveInfoStatus.department} -- 保持原值
        ELSE department -- 保持原值
        END,
        archiver = CASE
        WHEN archive_status = 0 THEN #{archiveInfoStatus.archiver} -- 修改归档人
        WHEN archive_status = 1 THEN #{archiveInfoStatus.archiver} -- 保持原值
        ELSE archiver -- 保持原值
        END
        WHERE id IN
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </update>
    <update id="sendArchiveInfo">
        update archive_info
        SET
        archive_status = case
        WHEN archive_status = 1 THEN 2 -- 发送利用
        WHEN archive_status = 2 THEN 1 -- 退回归档
        ELSE archive_status -- 其他状态保持原值
        END
        where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </update>

